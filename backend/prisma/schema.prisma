generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model File {
  id        String     @id @default(uuid())
  title     String     @unique
  resources Resource[]
}

model Resource {
  id      String @id @default(uuid())
  content String
  fileId  String
  file    File   @relation(fields: [fileId], references: [id])
}
